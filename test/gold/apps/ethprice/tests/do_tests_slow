set PATH=$PATH:/Users/jrush/source

echo "test5 - freshen default freq (120-two hours)"    2>&1 | tee test5
ethprice -t -f -v                                      >>test5 2>&1

echo "test6 - freshen different freq (240-four hours)" 2>&1 | tee test6
ethprice -t -f -p:240 -o -v                            >>test6 2>&1

echo "test7 - freshen different freq (1440-24 hours)"  2>&1 | tee test7
ethprice -t -f -p:1440 -o -v                           >>test7 2>&1

echo "test9 - test for -tod option error"              2>&1 | tee test9
ethprice -t -f -w:12 -o -v                             >>test9 2>&1

echo "test10 - test for -tod option error"             2>&1 | tee test10
ethprice -t -f -w:12 -p:120 -o -v                      >>test10 2>&1

echo "test11 - test for -tod option no error"          2>&1 | tee test11
ethprice -t -f -w:12 -p:1440 -o -v                     >>test11 2>&1

exit 0
